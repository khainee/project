                        android:textSize="16sp"
                        android:textColor="#000000"
                        android:textColorHint="#78909c"
                        android:gravity="center"
                        android:id="@+id/nrc1"
                        android:background="#ffffff"
                        android:padding="8dp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:text="၂ . ၆ . ၂၀၀၀"
                        android:hint="၈/စလန(နိင်)123456"
                        android:layout_weight="1"/>
                    <TextView
                        android:textSize="16sp"
                        android:textColor="#ffffff"
                        android:id="@+id/textview18"
                        android:padding="8dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="လူမျိုးဘာသာ."/>
                    <EditText
                        android:textSize="16sp"
                        android:textColor="#000000"
                        android:textColorHint="#78909c"
                        android:gravity="center"
                        android:id="@+id/nrc2"
                        android:background="#ffffff"
                        android:padding="8dp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:text="ဗမာ / ဗုဒ္ဓ"
                        android:hint="8/SALANA(N)123456"/>
                    <TextView
                        android:textSize="16sp"
                        android:textColor="#ffffff"
                        android:id="@+id/textview6"
                        android:padding="8dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="အရပ်."/>
                    <EditText
                        android:textSize="16sp"
                        android:textColor="#000000"
                        android:textColorHint="#78909c"
                        android:gravity="center"
                        android:id="@+id/birth1"
                        android:background="#ffffff"
                        android:padding="8dp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:text="၅&apos; ၆&apos;&apos;"
                        android:hint="၂-၅-၂၀၀၀"/>
                    <TextView
                        android:textSize="16sp"
                        android:textColor="#ffffff"
                        android:id="@+id/textview7"
                        android:padding="8dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="သွေးအုပ်စု."/>
                    <EditText
                        android:textSize="16sp"
                        android:textColor="#000000"
                        android:textColorHint="#78909c"
                        android:gravity="center"
                        android:id="@+id/birth2"
                        android:background="#ffffff"
                        android:padding="8dp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:text="အို"
                        android:hint="2-5-2000"/>
                    <TextView
                        android:textSize="16sp"
                        android:textColor="#ffffff"
                        android:id="@+id/textview8"
                        android:padding="8dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="အမှတ်."/>
                    <EditText
                        android:textSize="16sp"
                        android:textColor="#000000"
                        android:textColorHint="#78909c"
                        android:gravity="center"
                        android:id="@+id/type1"
                        android:background="#ffffff"
                        android:padding="8dp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:text="ဝဲးမျက်လုံး"
                        android:hint="အို"/>
                    <TextView
                        android:textSize="18sp"
                        android:textColor="#ffffff"
                        android:id="@+id/textview20"
                        android:padding="8dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="အသား."/>
                    <EditText
                        android:textSize="16sp"
                        android:textColor="#000000"
                        android:textColorHint="#78909c"
                        android:gravity="center"
                        android:id="@+id/edittext1"
                        android:background="#ffffff"
                        android:padding="8dp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:text="အောက်မှဲ့ရှိ"
                        android:hint="အို"/>
                    <TextView
                        android:textSize="18sp"
                        android:textColor="#ffffff"
                        android:id="@+id/textview21"
                        android:padding="8dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="အလုပ်အကိုင်"/>
                    <EditText
                        android:textSize="16sp"
                        android:textColor="#000000"
                        android:textColorHint="#78909c"
                        android:gravity="center"
                        android:id="@+id/juty"
                        android:background="#ffffff"
                        android:padding="8dp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:text="မှီခို"
                        android:hint="ခ/၁၂၃၄၅/၂၄"/>
                    <TextView
                        android:textSize="18sp"
                        android:textColor="#ffffff"
                        android:id="@+id/textview22"
                        android:padding="8dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="နေရပ်လိပ်စာ"/>
                    <EditText
                        android:textSize="16sp"
                        android:textColor="#000000"
                        android:textColorHint="#78909c"
                        android:gravity="center"
                        android:id="@+id/lacer"
                        android:background="#ffffff"
                        android:padding="8dp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:text="ကမာအိုင်ရွာ"
                        android:hint="ခ/၁၂၃၄၅/၂၄"/>
                    <TextView
                        android:textSize="18sp"
                        android:textColor="#ffffff"
                        android:id="@+id/textview23"
                        android:padding="8dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="မြို့နယ်"/>
                    <EditText
                        android:textSize="16sp"
                        android:textColor="#000000"
                        android:textColorHint="#78909c"
                        android:gravity="center"
                        android:id="@+id/myout"
                        android:background="#ffffff"
                        android:padding="8dp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:text="သရက်ချောင်းမြို့နယ်"
                        android:hint="ခ/၁၂၃၄၅/၂၄"/>
                    <TextView
                        android:textSize="18sp"
                        android:textColor="#ffffff"
                        android:id="@+id/textview24"
                        android:padding="8dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="လက်မှတ်"/>
                    <EditText
                        android:textSize="16sp"
                        android:textColor="#000000"
                        android:textColorHint="#78909c"
                        android:gravity="center"
                        android:id="@+id/Sign"
                        android:background="#ffffff"
                        android:padding="8dp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:text="Zaw"
                        android:hint="ခ/၁၂၃၄၅/၂၄"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:id="@+id/linear17"
                    android:padding="8dp"
                    android:layout_width="match_parent"
                    android:layout_height="10dp"/>
                <LinearLayout
                    android:orientation="horizontal"
                    android:id="@+id/linear18"
                    android:padding="8dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <Button
                        android:textSize="14sp"
                        android:textColor="#ffffff"
                        android:id="@+id/button1"
                        android:padding="8dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Done"/>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</LinearLayout>
