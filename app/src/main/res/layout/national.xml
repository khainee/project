                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="လိုင်စ​င်ဓာတ်ပုံ"/>
                    </LinearLayout>
                    <TextView
                        android:textSize="12sp"
                        android:textColor="#000000"
                        android:id="@+id/keying"
                        android:padding="8dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="TextView"/>
                    <TextView
                        android:textSize="12sp"
                        android:textColor="#000000"
                        android:id="@+id/filename"
                        android:padding="8dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textSize="12sp"
                        android:textColor="#f44336"
                        android:id="@+id/textview2"
                        android:padding="8dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="အမည်ရှေ့တွင် (ဦး၊ဒေါ် ၊ကို၊မောင် ၊မ) ထည့်ပါ။"/>
                    <LinearLayout
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:id="@+id/linear9"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:orientation="horizontal"
                            android:id="@+id/linear168"
                            android:padding="8dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <TextView
                                android:textSize="12sp"
                                android:textColor="#000000"
                                android:id="@+id/editText"
                                android:padding="8dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <TextView
                                android:textSize="12sp"
                                android:textColor="#000000"
                                android:id="@+id/textView"
                                android:padding="8dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <TextView
                                android:textSize="12sp"
                                android:textColor="#000000"
                                android:id="@+id/resect"
                                android:padding="8dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <TextView
                                android:textSize="12sp"
                                android:textColor="#000000"
                                android:id="@+id/key"
                                android:padding="8dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
